#!/usr/bin/env bash

LOGFILE=/var/log/logio/server.log
# Update the following line with the path to log.io-harvester
COMMAND=/home/logio/node_modules/log.io/bin/log.io-server
PIDFILE=/var/run/logio/log.io-server.pid

nohup $COMMAND >> $LOGFILE 2>&1 &
STATUS=$?
if [ $STATUS -eq 0 ]; then
    echo $! > $PIDFILE
fi
exit $STATUS
